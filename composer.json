{
  "name": "unity/config",
  "description": "Extensible configuration manager for PHP projects that uses dot notation and supports json, ini, array based and many more common formats",
  "type": "library",
  "keywords": [ "unity-framework", "unity", "php", "config", "configuration", "extensible"],
  "homepage": "https://github.com/unity-framework/Config",
  "license": "MIT",
  "authors": [
    {
      "name": "Eleandro Duzentos",
      "email": "eleandro@inbox.ru",
      "homepage": "https://github.com/e200",
      "role": "developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "require": {
    "unity/container": "dev-master",
    "unity/support": "dev-master",
    "unity/notator": "dev-master",
    "unity/contracts": "dev-master",
    "make/accessible": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "../container"
    },
    {
      "type": "vcs",
      "url": "../reflector"
    },
    {
      "type": "vcs",
      "url": "../contracts"
    },
    {
      "type": "vcs",
      "url": "../support"
    },
    {
      "type": "vcs",
      "url": "../notator"
    },
    {
      "type": "vcs",
      "url": "../make_accessible"
    }
  ],
  "autoload": {
    "psr-4": {
      "Unity\\Component\\Config\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unity\\Tests\\Config\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "mikey179/vfsStream": "^1.6",
    "mustangostang/spyc": "^0.6.2"
  },
  "suggests": {
    "mustangostang/spyc": "Enables yaml support"
  }
}
