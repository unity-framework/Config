{
  "name": "unity/config",
  "version": "0.1",
  "description": "Extensible configuration manager for PHP projects that uses dot notation and supports json, ini, array based and many more common formats",
  "type": "library",
  "keywords": [ "unity-framework", "unity", "php", "config", "configuration", "extensible"],
  "homepage": "https://github.com/unity-framework/Config",
  "license": "MIT",
  "authors": [
    {
      "name": "Eleandro Duzentos",
      "email": "eleandro@inbox.ru",
      "homepage": "https://github.com/e200",
      "role": "developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable" : true,
  "require": {
    "unity/notator": "^0.1.0",
    "make/accessible": "^0.1.0",
    "unity/support": "^0.1.0",
    "unity/container": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "Unity\\Component\\Config\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unity\\Tests\\Config\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "mikey179/vfsStream": "^1.6",
    "mustangostang/spyc": "^0.6.2"
  },
  "suggests": {
    "mustangostang/spyc": "Enables yaml support"
  }
}
